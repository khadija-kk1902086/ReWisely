{
  "compilerOptions": {
    "target": "es5",
  
      //"target": "es6",
     // "module": "commonjs",
   
 
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
  "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },

  "include": ["cypress","next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/mind-map/page.jsx", "app/loading.jsx", "app/mind-map/loading.jsx", "app/text-summary/loading.jsx", "app/page.test.tsx", "app/api/deleteMaterial/route.js", "app/[user]/[type]/[id]/page.tsx", "components/EditsSaving.jsx"],
  "exclude": ["node_modules"]
}
/* "moduleResolution": "bundler" */